{
    "name":         "unarealidad/canarium-modules-settings",
    "type":         "library",
    "description":  "Adds a ui interface for modifying canarium core's settings and other settings that customizes the site.",
    "keywords":     ["canarium", "una realidad"],
    "homepage":     "http://www.canarium.ph",
    "license":      "MIT",
    "authors": [
        {
            "name": "Jethro Laviste.",
            "email": "jethro.laviste@unarealidad.com",
            "role": "developer",
            "homepage": "http://packages.canarium.ph"
        }
    ],
    "support": {
        "email":    "errol.zuniga@unarealidad.com"
    },

    "minimum-stability": "stable",

    "require": {
        "unarealidad/canarium-kernal-core" : "dev-master"
    },

    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.canarium.ph"
        }
    ],

    "autoload": {
        "psr-0": {
            "Settings\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }

}
